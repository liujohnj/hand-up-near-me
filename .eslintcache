nt() {\r\n    axios.get('http://localhost:5000/providers/list')\r\n      .then(response => {\r\n        this.setState({ providers: response.data })\r\n      })\r\n      .catch((error) => {\r\n        console.log(error);\r\n      })\r\n  }\r\n  \r\n  deleteProvider(id) {\r\n    //delete document\r\n    axios.delete('http://localhost:5000/providers/'+id)\r\n      .then(response => console.log(response.data));\r\n      \r\n    //update page view with new deleted state\r\n    this.setState({\r\n      providers: this.state.providers.filter(el => el._id !== id)\r\n    })\r\n  }\r\n  \r\n  providerList() {\r\n    return this.state.providers.map(currentprovider => {\r\n      return <Provider provider={currentprovider} deleteProvider={this.deleteProvider} key={currentprovider._id}/>;\r\n    })\r\n  }\r\n  \r\n  render() {\r\n    return (\r\n      <div>\r\n        <h3>Providers</h3>\r\n        <table className=\"table\">\r\n          <thead className=\"thead-light\">\r\n            <tr>\r\n              <th>Username</th>\r\n              <th>Organization Name</th>\r\n              <th>Website?</th>\r\n              <th>URL</th>\r\n              <th>ZIP code</th>\r\n              <th>Actions</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            { this.providerList() }\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n    )\r\n  }\r\n}","C:\\Users\\john\\Projects\\hand-up\\mern-hand-up\\src\\components\\create-provider.component.js",[],"C:\\Users\\john\\Projects\\hand-up\\mern-hand-up\\src\\components\\read-users.component.js",["45"],"import React, { Component } from 'react';\r\nimport { Link } from 'react-router-dom';\r\nimport axios from 'axios';\r\n\r\n// Functional component as opposed to class component; could be its own file\r\nconst User = props => (\r\n  <tr>\r\n    <td>{props.user.username}</td>\r\n    <td>{props.user.firstName}</td>\r\n    <td>{props.user.lastName}</td>\r\n    <td>\r\n      <Link to={\"/users/update/\"+props.user._id}>update</Link> | <a href=\"#\" onClick={() => { props.deleteUser(props.user._id) }}>delete</a>\r\n    </td>\r\n  </tr>\r\n)\r\n\r\nexport default class UsersList extends Component {\r\n  constructor(props) {\r\n    super(props);\r\n\r\n    this.deleteUser = this.deleteUser.bind(this);\r\n\r\n    this.state = {users: []}\r\n  }\r\n\r\n  componentDidMount() {\r\n    console.log(\"Getting users from read-users.component!\");\r\n    axios.get('http://localhost:5000/users/list')\r\n      .then(response => {\r\n        this.setState({ users: response.data })\r\n      })\r\n      .catch((error) => {\r\n        console.log(error);\r\n      })\r\n  }\r\n  \r\n  deleteUser(id) {\r\n    //delete document\r\n    axios.delete('http://localhost:5000/users/'+id)\r\n      .then(response => console.log(response.data));\r\n      \r\n    //update page view with new deleted state\r\n    this.setState({\r\n      users: this.state.users.filter(el => el._id !== id)\r\n    })\r\n  }\r\n  \r\n  userList() {\r\n    return this.state.users.map(currentuser => {\r\n      return <User user={currentuser} deleteUser={this.deleteUser} key={currentuser._id}/>;\r\n    })\r\n  }\r\n  \r\n  render() {\r\n    return (\r\n      <div>\r\n        <h3>Users</h3>\r\n        <table className=\"table\">\r\n          <thead className=\"thead-light\">\r\n            <tr>\r\n              <th>Username</th>\r\n              <th>First name</th>\r\n              <th>Last name</th>\r\n              <th>Actions</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            { this.userList() }\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n    )\r\n  }\r\n}",["46","47"],"C:\\Users\\john\\Projects\\hand-up\\mern-hand-up\\src\\components\\update-user.component.js",[],"C:\\Users\\john\\Projects\\hand-up\\mern-hand-up\\src\\components\\create-user.component.js",[],"C:\\Users\\john\\Projects\\hand-up\\mern-hand-up\\src\\components\\update-provider.component.js",[],{"ruleId":"48","re